
"svg-scalable": {
	"prefix": "svg-scalable",
	"body": [ 
		"<svg xmlns:xlink='http://www.w3.org/1999/xlink' xmlns='http://www.w3.org/2000/svg'",
			"\tviewBox='$1' width='$2' height='$3' aria-labelledby='title desc'>",
		"",
			"\t<title id='title'>$4</title>",
			"\t<desc id='desc'>$5</desc>",
		"",
		"</svg>"
	],
	"description": "SVG - STRUCTURE: svg + title + desc"
}